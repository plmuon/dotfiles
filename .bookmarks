function hb5 {
  f=$(rmext "$1")
  HandBrakeCLI --main-feature --preset-import-gui  --preset 'plm h265' -i "$1" -o "$f".mp4
}

function hb5d {
  f=$(rmext "$1")
  HandBrakeCLI --main-feature --preset-import-gui  --preset 'plm h265 dfast' -i "$1" -o "$f".mp4
}
